{
  "name": "mdoback.front-projects",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "node server.js",
    "build-configuration:ua": "ng build --base-href /services/ua/ --outputPath dist/ua --aot",
    "build-configuration:ru": "ng build --base-href /services/ru/ --configuration=ru",
    "build-production:ru": "ng build --base-href /services/ru/ --configuration=production-ru",
    "build-production:ua": "ng build --prod --base-href /services/ua/ --outputPath dist/ua --aot",
    "build-production:all": "concurrently \"npm run build-production:ua\" \"npm run build-production:ru\"",
    "xi18n": "ng xi18n --out-file source.xlf --i18n-locale uk --output-path i18n",
    "merge": "xliffmerge --profile xliffmerge.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.1",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "6.0.1",
    "@angular/http": "6.0.1",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "6.0.1",
    "@angular/platform-browser-dynamic": "6.0.1",
    "@angular/router": "6.0.1",
    "ajv": "^6.5.0",
    "angular2-uuid": "^1.1.1",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.6",
    "express": "^4.16.3",
    "jasmine": "^3.1.0",
    "ngx-clipboard": "^11.0.0",
    "ngx-i18nsupport": "^0.16.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.2.1",
    "ts-md5": "^1.2.4",
    "xlsx": "^0.13.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "6.0.1",
    "@angular/compiler-cli": "6.0.1",
    "@angular/language-service": "6.0.1",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.110",
    "codelyzer": "^4.3.0",
    "hoxy": "^3.2.2",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "protractor": "^5.3.2",
    "ts-node": "^6.0.3",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  }
}
